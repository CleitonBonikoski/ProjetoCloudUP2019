@model IEnumerable<ProjetoCloud.Models.Ambiente>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nome_Ambiente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Data_Cadastro_Ambiente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Qtda_Dispositivo_Ambiente)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nome_Ambiente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Data_Cadastro_Ambiente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Qtda_Dispositivo_Ambiente)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id_Ambiente">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id_Ambiente">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id_Ambiente">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
